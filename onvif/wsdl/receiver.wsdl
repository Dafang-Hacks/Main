				<xs:attribute name="MaximumRTSPURILength" type="xs:int">
					<xs:annotation>
						<xs:documentation>The maximum allowed length for RTSP URIs (Minimum and default value is 128 octet).</xs:documentation>
					</xs:annotation>
				</xs:attribute>
