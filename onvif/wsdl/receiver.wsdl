				<xs:complexType>
					<xs:sequence>
						<xs:element name="ReceiverToken" type="tt:ReferenceToken">
							<xs:annotation>
								<xs:documentation>The token of the receiver to be configured.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Configuration" type="tt:ReceiverConfiguration">
							<xs:annotation>
